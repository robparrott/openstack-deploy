[DEFAULT]
devices = /mnt/{{ gv0 }}
mount_check = true
bind_port = 6011
user = {{ swift_user }}
log_facility = LOG_LOCAL2

[pipeline:main]
pipeline = container-server

[app:container-server]
use = egg:gluster_swift_ufo#container

[container-replicator]
vm_test_mode = yes

[container-updater]

[container-auditor]

[container-sync]